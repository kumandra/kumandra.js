{
  "name": "@kumandra/kumandra",
  "version": "0.2.0",
  "license": "Apache 2.0",
  "description": "This JavaScript library provides a simple API to submit and retrieve user data to the Kumandra network.",
  "main": "./dist/cjs/kumandra.js",
  "types": "./dist/cjs/kumandra.d.ts",
  "module": "./dist/esm/kumandra.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "rm -rf dist && npm run build:esm && npm run build:cjs",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:esm": "tsc --module esnext --outDir dist/esm",
    "lint": "eslint . --ext .ts",
    "test": "echo \"no test specified yet\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kumandra/kumandra.js.git"
  },
  "author": "Hangsia@kumandra.network>",
  "dependencies": {
    "@polkadot/api": "^7.7.1",
    "@polkadot/extension-dapp": "^0.42.7",
    "@polkadot/types": "^7.7.1",
    "@polkadot/ui-keyring": "^1.0.1",
    "@polkadot/util": "^8.3.3",
    "@polkadot/util-crypto": "^8.3.3"
  },
  "devDependencies": {
    "typescript": "^4.5.4",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "eslint-plugin-eslint-plugin": "^3.5.3"
  }
}
